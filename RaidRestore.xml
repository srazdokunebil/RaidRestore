	<!-- Minimap button -->
	<Button name="RaidRestore_RaidSelect" frameStrata="MEDIUM" parent="Minimap" enableMouse="true" movable="true" hidden="true">
		<Size>
			<AbsDimension x="36" y="36"/>
		</Size>
		<Anchors>
			<Anchor point="BOTTOMLEFT">
				<Offset>
					<AbsDimension x="-50" y="-50"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="RaidRestore_RaidSelectIcon" file="Interface\Icons\INV_Banner_02">
					<Size>
						<AbsDimension x="21" y="21"/>
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="8" y="-8"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY" setAllPoints="true">
				<Texture file="Interface\Minimap\MiniMap-TrackingBorder">
					<Size>
						<AbsDimension x="56" y="56"/>
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		
		<Scripts>
		    <OnEnter>
                GameTooltip:SetOwner(this, "ANCHOR_LEFT");
                GameTooltip:AddLine("Raid Restore v1.10");
				GameTooltip:AddLine("Left click to open options", 1, 1, 1)
				GameTooltip:AddLine("Right click to access quick select", 1, 1, 1)
                GameTooltip:Show();
            </OnEnter>
            <OnLeave>
                GameTooltip:Hide();
            </OnLeave>
			<OnMouseDown>
				if (arg1 == "LeftButton") then
					this:StartMoving();
				elseif (arg1 == "RightButton") then
					if (IsShiftKeyDown()) then
						ToggleDropDownMenu(1, nil, QuickAccess_FilterMenu, this:GetName(), -75, 15);
					else
						RaidMenuParentButton = nil;
						ToggleDropDownMenu(1, nil, QuickAccess_RaidMenu, this:GetName(), -135, 15);
					end
				end
			</OnMouseDown>
			<OnMouseUp>
				if (arg1 == "LeftButton") then
					this:StopMovingOrSizing();
				end
			</OnMouseUp>
			<OnShow>
				RaidRestore.SetOption("SelectVisible", 1);
			</OnShow>
			<OnHide>
				RaidRestore.SetOption("SelectVisible", 0);
			</OnHide>
		</Scripts>
	</Button>

